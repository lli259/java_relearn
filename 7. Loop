//while
int i = 0;
while (i < 5) {
  System.out.println(i);
  i++;
}

//for
for (int i = 0; i < 5; i++) {
  System.out.println(i);
}


//array loop, for

String[] cars = {"Volvo", "BMW", "Ford", "Mazda"};
for (String i : cars) {
  System.out.println(i);
}

//continue; to jump to next loop
//break; to jump out of loop
